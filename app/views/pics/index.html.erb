<div class="col-md-6 col-md-offset-3">
  <div id="pics" class='transitions-enabled'>
    <% @pics.each do |pic| %>
      <div class="box pannel pannel-default">
        <%= link_to (image_tag pic.image.url(:medium)), pic%>
        <h3><%= link_to pic.title, pic %></h3>
      </div>
    <% end %>
  </div>

</div>
